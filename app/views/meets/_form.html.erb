<%= form_for @meet, url: meets_path, :html => {:multipart => true} do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :date %><br />
    <%= f.text_field :date, "data-provide" => 'datepicker' %>
  </p>
  <p>
    <%= f.file_field :input_file %>
  </p>
  <p><%= f.submit %></p>
<% end %>