<div class=meet>
  <h1><%= @meet.name %> - <%= @meet.date %></h1>
  <% @results.each do | course, results | %>
    <% if results.size > 0 %>
      <h2><%= course %> Results</h2>
      
        <table %>
        <tr><th>Name</th><th>Place</th><th>Classifier</th><th>Time</th></tr>
          <% results.each do |r| %>
            <tr><td class=name><%= Runner.find(r.runner_id).name %></td><td><%= r.place%></td><td><%= r.classifier %></td><td><%= r.float_time.round(2) %></tr>
          <% end %>
        </table>
    <% end %>
  <% end %>
</div>
